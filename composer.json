{
  "name": "andrewmackrodt/phpmd-ruleset-builder",
  "type": "project",
  "description": "A tool to quickly create PHPMD rulesets",
  "license": "MIT",
  "keywords": [
    "phpmd",
    "pmd.xml"
  ],
  "authors": [
    {
      "name": "Andrew Mackrodt",
      "email": "hello@andrewmackrodt.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "mrclay/minify": "^3.0",
    "phpmd/phpmd": "^2.0",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true,
  "scripts": {
    "start": [
      "@php  -S 0.0.0.0:8080 router.php"
    ]
  }
}
